{
    "editor.fontSize": 20,
    "cmake.configureOnOpen": true,
    "remote.SSH.remotePlatform": {
        "192.168.230.214": "linux"
    },
    "git.ignoreLegacyWarning": true,
    "vim.operatorPendingModeKeyBindingsNonRecursive": [
    ],
    "vim.commandLineModeKeyBindingsNonRecursive": [
    ],
  "vim.sneak": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.camelCaseMotion.enable": true,
  "vim.highlightedyank.enable": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.surround": true,
  
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },       
    {
      "before": ["<leader>","s" ], 
      "commands": [ ":w!" ]
    },
    {
      "before": [ "<leader>", "q" ],
      "commands": [ ":q!" ]
    },
    {
      "before": [ "<leader>", "sq" ],
      "commands":[":wq!"]
  },
  {
    "before": [ "L" ],
    "after": [ "$" ]
  },
  {
    "before": [ "H" ],
    "after": [ "^" ]
  },
  {
    "before": [ "*" ],
    "after": [ "*", "z", "z" ]
  },
  {
    "before": ["<leader>", "y"],
    "after": ["v","a","w","y"]
  },
  {
    "before": ["<leader>", "p"],
    "after": ["v","a","w","p"]
  },
  {
    "before": [ ";" ],
    "after": [ ":" ]
  }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.commandLineModeKeyBindings": [
  
  ],
  "workbench.settings.openDefaultKeybindings": true,

}